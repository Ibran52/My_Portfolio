<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Portfolio</title>
    <link rel="stylesheet" href="/stylesheet/navbaar.css">
</head>

<body>
    <header>
        <!-- <div class="brand-name">Ibran Siddique</div> -->
        <nav>
            <div class="menu-icon" id="menu-toggle">
                <i class="ri-menu-3-line"></i>
            </div>
            <ul class="text" id="nav-menu">
                <% data.navigation.forEach(item=> { %>
                    <li><a href="<%= item.link %>">
                            <%= item.label %>
                        </a></li>
                    <% }); %>
                        <li class="cursor-toggle" style="cursor: pointer; margin-left: 10px;"
                            title="Toggle Cursor Effect"><i class="ri-mouse-fill" id="cursor-icon"></i></li>
            </ul>
        </nav>
    </header>

    <svg style="display: none">
        <defs>
            <filter id="wave-distort" x="0%" y="0%" width="100%" height="100%">
                <feTurbulence type="fractalNoise" baseFrequency="0.0038" numOctaves="1" seed="2" result="noise" />
                <feGaussianBlur in="noise" stdDeviation="8.5" result="blur" />
                <feComposite operator="arithmetic" k2="1" k3="2" in="blur" />
                <feDisplacementMap in="SourceGraphic" in2="blur" scale="-42" xChannelSelector="G"
                    yChannelSelector="G" />
            </filter>
        </defs>
    </svg>
</body>
<script type="module" src="/javascript/navbaar.js"></script>

</html>